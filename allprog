va import
javax.servlet.http.*; import
javax.servlet.*; import
java.io.*;
public class ServletRequest extends HttpServlet{
public void doGet(HttpServletRequestreq,HttpServletResponse res) throws
ServletException,IOException
{
res.setContentType("text/html");
PrintWriterpwriter=res.getWriter(); String
name = req.getParameter("uname"); String
age = req.getParameter("uage");
pwriter.println("!Welcome: "+name);
