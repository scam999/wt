 <html>
 <body>
<?php
$con = mysqli_connect('localhost','root','','mydb');
if(!$con)
die('Error in Connection'.mysqli_error());
$sel = mysqli_select_db($con, 'mydb');
if(!$sel)
echo "<br> Database not selected";
$name = $_POST['un'];
$pwd = $_POST['pwd'];
$qr = mysqli_query($con, "SELECT COUNT(1) from login1 where uname = '$name'");
$row = mysqli_fetch_array($qr);
if($row[0] >= 1){
echo "
<script>
alert('User Already exists');
window.location.href = 'login.php';
 </script>";
}
else{
$qry2 = "INSERT INTO login1(uname,pword) VALUES('$name','$pwd')";
$ins = mysqli_query($con, $qry2);
if(!$ins)
echo"<br>Login data Not inserted successfully";
echo "<br> User $name data inserted successfully";
echo "<br><a href = 'login.php'>Move to Home page</a>";
}
mysqli_close($con);
?>
 </body>
 </html>
 OUTPUT :
 <!—Retriving Users -Displaying users data : displayusers.php-->
 <html>
 <body>
<?php
$con = mysqli_connect('localhost','root','','mydb');
if(!$con)
{
die('Error in connection'.mysqli_error());
}
$sel = mysqli_select_db($con, 'mydb');
if(!$sel)
echo"<br> Database not selected";
$res = mysqli_query($con, "SELECT * from login1");
echo"<center>";
echo"<table border = 2>";
echo "<caption> Users Details</caption>";
echo "<tr><th>USER NAME</th><th>PASSWORD</th></tr>";
while($row = mysqli_fetch_array($res))
{ echo "<tr><td>";
echo $row['uname'];
echo "</td>";
echo "<td>";
echo $row['pword'];
echo "</td></tr>";}
echo "</table>";
echo "</center>";
echo "<br><a href = 'login.php'>Move to Home Page</a>";
mysqli_close($con);?>
 </body>
 </html>
